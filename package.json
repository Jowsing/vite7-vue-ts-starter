    "@types/js-cookie": "^3.0.6",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.33.0",
    "eslint-plugin-html": "^8.1.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.3.0",
    "husky": "^8.0.0",
    "lint-staged": "^16.1.5",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
