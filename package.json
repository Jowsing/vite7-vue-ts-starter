  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "engines": {
    "node": ">=24.x",
    "pnpm": ">=9.x"
  },
  "keywords": [
    "vue",
    "vue3",
    "vue-next",
    "vite",
    "vite7",
    "jest",
    "vitest",
    "typescript",
    "example",
    "template",
    "element-plus"
  ],
  "homepage": "https://github.com/jowsing/vite7-vue-ts-starter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jowsing/vite7-vue-ts-starter"
  },
  "bugs": {
    "url": "https://github.com/jowsing/vite7-vue-ts-starter/issues"
  },
  "dependencies": {
    "@vueuse/core": "^13.7.0",
    "axios": "^1.11.0",
    "element-plus": "^2.10.7",
